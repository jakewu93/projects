   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "mean: 2.8461538461538463 std: 0.7510676161988106 median: 3.0 min: 2 max: 4\n"
     ]
    }
   ],
   "source": [
    "Property_details_84100=Property_details.loc[Property_details.zipcode==84100,:]\n",
    "\n",
    "mean_starratings=Property_details_84100.starrating.mean()\n",
    "standard_deviation_starratings=Property_details_84100.starrating.std()\n",
    "median_starratings=Property_details_84100.starrating.median()\n",
    "min_starratings=Property_details_84100.starrating.min()\n",
    "max_starratings=Property_details_84100.starrating.max()\n",
    "\n",
    "print(\"mean:\",mean_starratings,\"std:\",standard_deviation_starratings,\"median:\",median_starratings,\"min:\",min_starratings,\"max:\",max_starratings)"
   ]
